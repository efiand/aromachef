{
	"browserslist": [
		"> 0.2%",
		"last 6 version"
	],
	"devDependencies": {
		"@dotenvx/dotenvx": "1.38.4",
		"@fastify/pre-commit": "2.2.0",
		"@prisma/client": "6.4.1",
		"@sveltejs/adapter-node": "5.2.12",
		"@sveltejs/amp": "1.1.4",
		"@sveltejs/kit": "2.19.0",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@types/node": "22.13.10",
		"cross-env": "7.0.3",
		"dayjs": "1.11.13",
		"eslint": "9.22.0",
		"eslint-config-prettier": "10.1.1",
		"eslint-plugin-perfectionist": "4.10.0",
		"eslint-plugin-svelte": "3.0.3",
		"globals": "16.0.0",
		"mime": "4.0.6",
		"postcss-html": "1.8.0",
		"postcss-scss": "4.0.9",
		"postcss-sort-media-queries": "5.2.0",
		"postcss-url": "10.1.3",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.3.3",
		"prisma": "6.4.1",
		"sass-embedded": "1.85.1",
		"stylelint": "16.15.0",
		"stylelint-config-standard": "37.0.0",
		"stylelint-order": "6.0.4",
		"svelte": "5.22.6",
		"svelte-check": "4.1.5",
		"typescript": "5.8.2",
		"typescript-eslint": "8.26.0",
		"vite": "6.2.1"
	},
	"pre-commit": [
		"lint-format"
	],
	"private": true,
	"scripts": {
		"build": "npm run check && vite build && cross-env AMP=1 vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev",
		"dump": "dotenvx run -- node tools/dump",
		"lint": "npm run check && eslint . && stylelint **/*.{scss,svelte} && prettier --check .",
		"lint-format": "npm run check && eslint . --fix && stylelint **/*.{scss,svelte} --fix && prettier --write .",
		"postinstall": "prisma generate && svelte-kit sync",
		"preview": "vite preview",
		"upgrade": "node tools/upgrade"
	},
	"type": "module"
}
