{
	"browserslist": [
		"> 0.2%",
		"last 6 version"
	],
	"devDependencies": {
		"@dotenvx/dotenvx": "1.42.2",
		"@fastify/pre-commit": "2.2.0",
		"@prisma/client": "6.7.0",
		"@sveltejs/adapter-node": "5.2.12",
		"@sveltejs/amp": "1.1.4",
		"@sveltejs/kit": "2.20.8",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@types/node": "22.15.3",
		"cross-env": "7.0.3",
		"dayjs": "1.11.13",
		"eslint": "9.26.0",
		"eslint-config-prettier": "10.1.2",
		"eslint-plugin-perfectionist": "4.12.3",
		"eslint-plugin-svelte": "3.5.1",
		"globals": "16.0.0",
		"mime": "4.0.7",
		"postcss-html": "1.8.0",
		"postcss-scss": "4.0.9",
		"postcss-sort-media-queries": "5.2.0",
		"postcss-url": "10.1.3",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.3.3",
		"prisma": "6.7.0",
		"sass-embedded": "1.87.0",
		"stylelint": "16.19.1",
		"stylelint-config-standard": "38.0.0",
		"stylelint-order": "7.0.0",
		"svelte": "5.28.2",
		"svelte-check": "4.1.7",
		"typescript": "5.8.3",
		"typescript-eslint": "8.31.1",
		"vite": "6.3.4"
	},
	"pre-commit": [
		"lint-format"
	],
	"private": true,
	"scripts": {
		"build": "npm run check && vite build && cross-env AMP=1 vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev",
		"dump": "dotenvx run -- node --experimental-transform-types tools/dump.ts",
		"lint": "npm run check && eslint . && stylelint \"**/*.{scss,svelte}\" && prettier --check .",
		"lint-format": "npm run check && eslint . --fix && stylelint \"**/*.{scss,svelte}\" --fix && prettier --write .",
		"postinstall": "prisma generate && svelte-kit sync",
		"preview": "vite preview",
		"upgrade": "node --experimental-transform-types tools/upgrade.ts"
	},
	"type": "module"
}
